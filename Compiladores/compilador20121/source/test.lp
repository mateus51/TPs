integer PROCEDURE fatorial(value integer:n):
  x, result : integer;
begin
  x := 1;
  result := 1;
  repeat
    result := result * x; 
    x := x + 1
  until x <= n;
  i := result
end

begin
  i := fatorial(2, 'a');
  x := fatorial(3);
  if 2.0 then write(1) else write('b');
  x := abs(12);
  x := abs(12.0);
